#sidebar
  %hr
  - if u.try(:info_full?)
    .row.menu
      - if go_back?
        = link_to request.headers['HTTP_REFERER'], remote: true, title: 'Go Back to page', class: 'u-pull-left' do
          = fa_icon 'reply 2x', class: 'black-color'
      -#= link_to '#', title: 'ArtWork' do
        = fa_icon 'gavel 2x', class: 'black-color'
      -#= link_to '#', title: 'Parts' do
        = fa_icon 'gears 2x', class: 'black-color'
      -#= link_to '#', title: 'Tools' do
        = fa_icon 'puzzle-piece 2x', class: 'black-color'
      -#= link_to '#', title: 'Articles' do
        = fa_icon 'eye 2x', class: 'black-color'
      -#= link_to '#', title: 'Events' do
        = fa_icon 'beer 2x', class: 'black-color'
      = link_to dashboard_musics_path, remote: true, title: 'Musics' do
        = fa_icon 'music 2x', class: 'black-color'
      -#= link_to '#', title: 'Videos' do
        = fa_icon 'file-video-o 2x', class: 'black-color'
      -#= link_to '#', title: 'Photos' do
        = fa_icon 'photo 2x', class: 'black-color'
      = link_to users_path, remote: true, title: 'Members' do
        = fa_icon 'users 2x', class: 'black-color'
  - else
    - if go_back?
      .row.menu.none
        = link_to request.headers['HTTP_REFERER'], remote: true, title: 'Go Back to page', class: 'u-pull-left' do
          = fa_icon 'reply 2x', class: 'black-color'
